{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ]
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "biteshare-ee6c8",
          "appId": "1:399319667627:android:bad72e2256b5984db578ee",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "biteshare-ee6c8",
          "appId": "1:399319667627:ios:39c51ce730ef4715b578ee",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "biteshare-ee6c8",
          "configurations": {
            "android": "1:399319667627:android:bad72e2256b5984db578ee",
            "ios": "1:399319667627:ios:39c51ce730ef4715b578ee",
            "web": "1:399319667627:web:175ae0af5e644486b578ee"
          }
        }
      }
    }
  }
}
